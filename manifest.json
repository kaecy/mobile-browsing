{
	"manifest_version": 2,
	"name": "Mobile Browsing",
	"version": "1.0",

	"description": "View pages as a mobile user.",

	"icons": {
		
	},
	
	"permissions": [
		"webRequest", "webRequestBlocking", "<all_urls>"
	],
	
	"background": {
		"scripts": ["background.js"]
	},
	
	"browser_action": {
		"default_icon": {},
		"default_title": "Mobile Browsing",
		"default_popup": "popup/config.html"
	}
}
